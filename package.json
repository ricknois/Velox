{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "./node_modules/.bin/eslint .",
    "eject": "expo eject",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo/android",
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@unimodules/.*|jest-expo/.*|@expo(nent)?/.*|react-navigation|@react-navigation/.*|sentry-expo|native-base))"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "@expo-google-fonts/inter": "^0.2.0",
    "@expo-google-fonts/rajdhani": "^0.2.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^6.0.4",
    "@react-navigation/native": "^6.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "expo": "^42.0.0",
    "expo-ads-admob": "~10.1.2",
    "expo-app-loading": "^1.1.2",
    "expo-constants": "^11.0.1",
    "expo-font": "~9.2.1",
    "expo-intent-launcher": "~9.1.0",
    "expo-linear-gradient": "~9.2.0",
    "expo-linking": "~2.3.1",
    "expo-location": "~12.1.2",
    "expo-modules-core": "^0.2.0",
    "expo-status-bar": "~1.0.4",
    "expo-store-review": "~4.1.0",
    "geolib": "^3.3.1",
    "i18n-js": "^3.8.0",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-web": "~0.17.1",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.15.4",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/styled-components-react-native": "^5.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "jest-expo": "^42.1.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2"
  },
  "private": true
}
