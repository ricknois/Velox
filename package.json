{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "./node_modules/.bin/eslint .",
    "eject": "expo eject",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo/android",
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@unimodules/.*|jest-expo/.*|@expo(nent)?/.*|react-navigation|@react-navigation/.*|sentry-expo|native-base))"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@babel/preset-react": "^7.14.5",
    "@expo-google-fonts/inter": "^0.2.0",
    "@expo-google-fonts/rajdhani": "^0.2.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/bottom-tabs": "^6.0.4",
    "@react-navigation/native": "^6.0.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "expo": "^50.0.8",
    "expo-app-loading": "^1.1.2",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.2",
    "expo-intent-launcher": "~10.11.0",
    "expo-linear-gradient": "~12.7.1",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.5.5",
    "expo-modules-core": "~1.11.8",
    "expo-status-bar": "~1.11.1",
    "expo-store-review": "~6.8.3",
    "geolib": "^3.3.1",
    "i18n-js": "^3.8.0",
    "moment": "^2.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^17.0.2",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-web": "~0.19.6",
    "styled-components": "^5.3.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/styled-components-react-native": "^5.1.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "jest-expo": "~50.0.2",
    "prettier": "^2.3.2"
  },
  "private": true
}
