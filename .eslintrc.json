{
    "env": {
        "es2021": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": "off",
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "no-nested-ternary": 0,
        "no-unused-expressions": 0
    }
}
